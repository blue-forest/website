{
  "name": "blueforest.cc",
  "version": "1.5.1.1",
  "author": "Nazim Lachter <nazim@blueforest.cc>",
  "repository": "git@github.com:theblueforest/blueforest.cc.git",
  "license": "Apache-2.0",
  "main": "./dist/index.html",
  "scripts": {
    "start": "kiwi start",
    "test": "kiwi test",
    "build": "kiwi build",
    "push": "kiwi build && docker build -t theblueforest/website:$npm_package_version . && docker push theblueforest/website:$npm_package_version"
  },
  "bundles": {
    "kiwi-bundle": {
      "options": {
        "app": {
          "name": "Blue Forest",
          "author": "Blue Forest",
          "description": "Blue Forest vous accompagne dans le design et l'implémentation d'expériences utilisateurs web et mobile."
        },
        "dev": {
          "webHost": "localhost",
          "webPort": 8040
        }
      },
      "handlers": {
        "client": "./client"
      }
    }
  },
  "dependencies": {
    "@types/react-google-recaptcha": "^1.1.1",
    "kiwi-bundle": "3.1.0",
    "kiwi-bundle-react": "1.1.0",
    "react-ga": "^2.7.0",
    "react-google-recaptcha": "^2.0.1"
  }
}
